{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "status": 200
      }
    ]
  }